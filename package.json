    "@babel/preset-env": "^7.16.8",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "ejs": "^3.1.6",
    "eslint": "^8.7.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
