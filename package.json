{
  "name": "template",
  "version": "1.0.0",
  "description": "template",
  "scripts": {
    "lint": "npm run lint:styles; npm run lint:scripts",
    "lint:styles": "stylelint src/styles",
    "lint:scripts": "eslint src/scripts",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:dev": "webpack --config webpack/webpack.config.prod.js",
    "start": "webpack serve --port 8888 --config webpack/webpack.config.dev.js",
    "deploy": "npm run build:dev && rsync -auvz -e 'ssh -i ~/.ssh/(secretKey)' build/* user@server:/var/www/html/dev/"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "kobataka0505",
